# Server Configuration
NODE_ENV=development
PORT=8721

# Storage Configuration
STORAGE_PATH=./storage

# File Upload Limits
MAX_FILE_SIZE_MB=50
MAX_JSONL_LINES=10000000

# cclogviewer Configuration
# Path to cclogviewer binary (assumes 'cclogviewer' is in PATH)
# If installed via 'go install', typically at ~/go/bin/cclogviewer
CCLOGVIEWER_BIN_PATH=cclogviewer
CCLOGVIEWER_TIMEOUT_MS=60000

# Rate Limiting
RATE_LIMIT_UPLOAD_WINDOW_MS=900000
RATE_LIMIT_UPLOAD_MAX=10
RATE_LIMIT_VIEW_WINDOW_MS=60000
RATE_LIMIT_VIEW_MAX=100

# Application URL
BASE_URL=http://localhost:8721
